  "components": {
    "schemas": {
      "LOGIN_REQUEST": {
        "type": "object",
        "required": [
          "email",
          "password"
        ],
        "properties": {
          "email": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      },
      "LOGIN_RESPONSE": {
        "type": "object",
        "properties": {
          "accessToken": {
            "type": "string"
          },
          "username": {
            "type": "string"
          },
          "accessList": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "PROFILE_RESPONSE": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "username": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "fullName": {
            "type": "string"
          }
        }
      },
      "BOOKS": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "title": {
            "type": "string"
          },
          "author": {
            "type": "string"
          },
          "category": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "publisher": {
            "type": "string"
          },
          "publishedDate": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "BOOKS_LIST_RESPONSE": {
        "type": "array",
        "items": {
          "$ref": "#/components/schemas/BOOKS"
        }
      }
    },
    "parameters": {
      "CLIENT_ID": {
        "name": "client-id",
        "in": "header",
        "required": true,
        "schema": {
          "type": "string"
        }
      },
      "CLIENT_SECRET": {
        "name": "secret",
        "in": "header",
        "required": true,
        "schema": {
          "type": "string"
        }
      }
    },
    "examples": {
      "LOGIN_SUCCESS": {
        "value": {
          "accessToken": "123321",
          "username": "fahmi sulaiman",
          "accessList": [
            "READ",
            "WRITE"
          ]
        }
      },
      "PROFILE_RESPONSE": {
