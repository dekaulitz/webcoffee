            "category": [
              "string"
            ],
            "publisher": "string",
            "publishedDate": "2020-10-10T14:51:46.311Z"
          }
        ]
      }
    },
    "securitySchemes": {
      "BearerAuth": {
        "type": "http",
        "scheme": "bearer",
        "bearerFormat": "JWT"
      }
    }
  }
